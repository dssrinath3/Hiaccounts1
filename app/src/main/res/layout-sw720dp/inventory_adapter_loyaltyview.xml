<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@drawable/rectangle_with_border">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:weightSum="3"
        android:orientation="horizontal">

        <android.support.v7.widget.CardView
            xmlns:card_view="http://schemas.android.com/apk/res-auto"
            android:id="@+id/card_view"
            android:layout_gravity="center"
            android:layout_height="match_parent"
            android:layout_width="0dp"
            android:layout_weight="1"
            card_view:cardCornerRadius="4dp"
            android:layout_margin="2dp">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">
                <TextView
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:gravity="center"
                    android:id="@+id/tv_TextloyaltyRedemptionprogram"
                    android:padding="1dp"
                    android:text="Redemption program"
                    android:textColor="@color/colorBlack"
                    android:textAppearance="?android:textAppearanceSmall"/>

                <TextView
                    android:layout_gravity="center_vertical"
                    android:layout_height="match_parent"
                    android:layout_width="match_parent"
                    android:gravity="center"
                    android:id="@+id/tv_loyaltyRedemptionprogram"
                    android:padding="1dp"
                    android:text=""
                    android:textAppearance="?android:textAppearanceSmall"/>
            </LinearLayout>



        </android.support.v7.widget.CardView>
        <android.support.v7.widget.CardView
            xmlns:card_view="http://schemas.android.com/apk/res-auto"
            android:id="@+id/card_view1"
            android:layout_gravity="center"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            card_view:cardCornerRadius="4dp"
            android:layout_margin="2dp">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">
                    <TextView
                        android:layout_gravity="center_vertical"
                        android:gravity="center"
                        android:id="@+id/tv_TextloyaltyCurrency"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:padding="1dp"
                        android:text="Currency"
                        android:textColor="@color/colorBlack"
                        android:textAppearance="?android:textAppearanceSmall"/>
                    <TextView
                        android:layout_gravity="center_vertical"
                        android:gravity="center"
                        android:id="@+id/tv_loyaltyCurrency"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:padding="1dp"
                        android:text=""
                        android:textAppearance="?android:textAppearanceSmall"/>
            </LinearLayout>
        </android.support.v7.widget.CardView>
        <android.support.v7.widget.CardView
            xmlns:card_view="http://schemas.android.com/apk/res-auto"
            android:id="@+id/card_view8"
            android:layout_gravity="center"
            android:layout_height="match_parent"
            android:layout_width="0dp"
            android:layout_weight="1"
            card_view:cardCornerRadius="4dp"
            android:layout_margin="2dp">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">
                    <TextView
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:gravity="center"
                    android:id="@+id/tv_TextloyaltyPointsPerCurrency"
                    android:padding="1dp"
                    android:text="Points Per Currency"
                    android:textAppearance="?android:textAppearanceSmall"
                        android:textColor="@color/colorBlack"/>
                <TextView
                    android:layout_gravity="center_vertical"
                    android:layout_height="match_parent"
                    android:layout_width="match_parent"
                    android:gravity="center"
                    android:id="@+id/tv_loyaltyPointsPerCurrency"
                    android:padding="1dp"
                    android:text=""
                    android:textAppearance="?android:textAppearanceSmall"/>
            </LinearLayout>

        </android.support.v7.widget.CardView>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:weightSum="3"
        android:orientation="horizontal">
        <android.support.v7.widget.CardView
            xmlns:card_view="http://schemas.android.com/apk/res-auto"
            android:id="@+id/card_view2"
            android:layout_gravity="center"
            android:layout_height="match_parent"
            android:layout_width="0dp"
            android:layout_weight="1"
            card_view:cardCornerRadius="4dp"
            android:layout_margin="2dp">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">
                <TextView
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:id="@+id/tv_TextloyaltyOrder"
                    android:layout_width="match_parent"
                    android:padding="1dp"
                    android:text="Order"
                    android:textAppearance="?android:textAppearanceSmall"
                    android:textColor="@color/colorBlack"/>
                <TextView
                    android:layout_gravity="center_vertical"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:id="@+id/tv_loyaltyOrder"
                    android:layout_width="match_parent"
                    android:padding="1dp"
                    android:text=""
                    android:textAppearance="?android:textAppearanceSmall"/>

            </LinearLayout>
        </android.support.v7.widget.CardView>
        <android.support.v7.widget.CardView
            xmlns:card_view="http://schemas.android.com/apk/res-auto"
            android:id="@+id/card_view3"
            android:layout_gravity="center"
            android:layout_height="match_parent"
            android:layout_width="0dp"
            android:layout_weight="1"
            card_view:cardCornerRadius="4dp"
            android:layout_margin="2dp">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">
                <TextView
                    android:layout_gravity="center_vertical"
                    android:gravity="center"
                    android:id="@+id/tv_TextloyaltyPointsPerOrder"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="1dp"
                    android:text="Points Per Order"
                    android:textColor="@color/colorBlack"
                    android:textAppearance="?android:textAppearanceSmall"/>
                <TextView
                    android:layout_gravity="center_vertical"
                    android:gravity="center"
                    android:id="@+id/tv_loyaltyPointsPerOrder"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:padding="1dp"
                    android:text=""
                    android:textAppearance="?android:textAppearanceSmall"/>

            </LinearLayout>
        </android.support.v7.widget.CardView>
        <android.support.v7.widget.CardView
            xmlns:card_view="http://schemas.android.com/apk/res-auto"
            android:id="@+id/card_view4"
            android:layout_gravity="center"
            android:layout_height="match_parent"
            android:layout_width="0dp"
            android:layout_weight="1"
            card_view:cardCornerRadius="4dp"
            android:layout_margin="2dp">

            <TextView
                android:layout_gravity="center_vertical"
                android:layout_height="match_parent"
                android:gravity="center"
                android:id="@+id/tv_LoyaltyEdit"
                android:layout_width="match_parent"
                android:padding="@dimen/tvpadding"
                android:textAppearance="?android:textAppearanceSmall" />

    </android.support.v7.widget.CardView>

    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:weightSum="3"
        android:orientation="horizontal">
        <android.support.v7.widget.CardView
            xmlns:card_view="http://schemas.android.com/apk/res-auto"
            android:id="@+id/card_view6"
            android:layout_gravity="center"
            android:layout_height="match_parent"
            android:layout_width="0dp"
            android:layout_weight="1"
            card_view:cardCornerRadius="4dp"
            android:layout_margin="2dp">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">
                    <TextView
                        android:layout_gravity="center_vertical"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:id="@+id/tv_TextloyaltyProduct"
                        android:layout_width="match_parent"
                        android:padding="1dp"
                        android:text="Product"
                        android:textColor="@color/colorBlack"
                        android:textAppearance="?android:textAppearanceSmall"/>
                <TextView
                    android:layout_gravity="center_vertical"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:id="@+id/tv_loyaltyProduct"
                    android:layout_width="match_parent"
                    android:padding="1dp"
                    android:text=""
                    android:textAppearance="?android:textAppearanceSmall"/>

            </LinearLayout>
        </android.support.v7.widget.CardView>
        <android.support.v7.widget.CardView
            xmlns:card_view="http://schemas.android.com/apk/res-auto"
            android:id="@+id/card_view7"
            android:layout_gravity="center"
            android:layout_height="match_parent"
            android:layout_width="0dp"
            android:layout_weight="1"
            card_view:cardCornerRadius="4dp"
            android:layout_margin="2dp">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">
                <TextView
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:id="@+id/tv_TextloyaltyPointsPerProduct"
                    android:layout_width="match_parent"
                    android:padding="1dp"
                    android:text="Points Per Product"
                    android:textColor="@color/colorBlack"
                    android:textAppearance="?android:textAppearanceSmall"/>
                <TextView
                    android:layout_gravity="center_vertical"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:id="@+id/tv_loyaltyPointsPerProduct"
                    android:layout_width="match_parent"
                    android:padding="1dp"
                    android:text=""
                    android:textAppearance="?android:textAppearanceSmall"/>
            </LinearLayout>
        </android.support.v7.widget.CardView>
        <android.support.v7.widget.CardView
            xmlns:card_view="http://schemas.android.com/apk/res-auto"
            android:id="@+id/card_view10"
            android:layout_gravity="center"
            android:layout_height="match_parent"
            android:layout_width="0dp"
            android:layout_weight="1"
            card_view:cardCornerRadius="4dp"
            android:layout_margin="2dp">

                <ImageView
                    android:visibility="visible"
                    android:layout_height="20dp"
                    android:layout_gravity="center"
                    android:id="@+id/imgviewEdit"
                    android:layout_width="20dp"
                    android:src="@drawable/ic_edit"
                    android:padding="@dimen/tvpadding"
                    android:textAppearance="?android:textAppearanceSmall" />


</android.support.v7.widget.CardView>

    </LinearLayout>
</LinearLayout>